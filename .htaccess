<IfModule mod_rewrite.c>
    RewriteEngine on
    RewriteCond %{HTTPS} !=on
    RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
    RewriteRule ^(.well-known) - [L]
    RewriteRule    ^$    public/    [L]
    RewriteRule    (.*) public/$1    [L]
SetEnvIf Authorization "(.*)" HTTP_AUTHORIZATION=$1

</IfModule>

